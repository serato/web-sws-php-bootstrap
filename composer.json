{
    "name": "serato/sws-bootstrap",
    "homepage": "https://github.com/serato/web-sws-php-bootstrap",
    "description": "Bootstrap functionality for SWS web applications",
    "type": "library",
    "require": {
        "php": "^8.3",
        "slim/slim": "^4.13.0",
        "serato/jwt": "dev-feature-php8",
        "aws/aws-sdk-php": "^3.0",
        "willdurand/negotiation": "^3.1.0",
        "geoip2/geoip2": "^2.0.0",
        "scrivo/highlight.php": "v9.12.0.4",
        "symfony/console": "^7.0",
        "ramsey/uuid": "^3.0.0",
        "rakit/validation": "^1.4",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0",
        "squizlabs/php_codesniffer": "~3.6",
        "monolog/monolog": "^1.21.0",
        "serato/slimulator": "dev-feature-php8",
        "symfony/cache": "^7.0",
        "enlightn/security-checker": "^1.4",
        "mockery/mockery": "^1.3",
        "propel/propel": "2.0.0-beta4",
        "rector/rector": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.54"
    },
    "autoload": {
        "psr-4": {
            "Serato\\SwsApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serato\\SwsApp\\Test\\": "tests/",
            "Serato\\SwsApp\\Test\\Propel\\Model\\": "tests/resources/propel/model",
            "Utils\\Rector\\": "utils/rector/src",
            "Utils\\Rector\\Tests\\": "utils/rector/tests"
        }
    }
}
